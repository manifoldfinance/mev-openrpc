{
    "name": "eth_sendBundle",
    "summary": "Used for submitting a proof-of-work solution.",
    "params": [{
            "$ref": "#/components/contentDescriptors/Nonce"
        },
        {
            "name": "revertingTxHashes",
            "type": "string",
            "description": "(Optional) Array[String], A list of tx hashes that are allowed to revert. Array is Keccak 256 Hash of the RLP encoding of a transaction",
            "required": false,
            "schema": {
                "$ref": "#/components/schemas/Keccak"
            }
        },
        {
            "name": "maxTimestamp",
            "required": true,
            "schema": {
                "$ref": "#/components/schemas/blockTimeStamp"
            }
        },
        {
            "name": "minTimestamp",
            "required": true,
            "schema": {
                "$ref": "#/components/schemas/blockTimeStamp"
            }
        }
    ],
    "result": {
        "name": "solutionValid",
        "description": "returns true if the provided solution is valid, otherwise false.",
        "schema": {
            "type": "boolean"
        }
    },
    "examples": [{
        "name": "eth_sendBundle POST",
        "jsonrpc": "2.0",
        "id": 1,
        "method": "eth_callBundle",
